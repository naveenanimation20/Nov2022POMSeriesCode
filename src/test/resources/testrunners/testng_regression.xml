<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Opencart App Test Regression Suite" thread-count="4"
	parallel="tests" verbose="4">


	<listeners>
		<listener
			class-name="com.qa.opencart.listeners.ExtentReportListener" />
		<listener
			class-name="com.qa.opencart.listeners.TestAllureListener" />

		<listener
			class-name="com.qa.opencart.listeners.AnnotationTransformer" />
	</listeners>

	<test name="Login Page Test">
	
		<classes>
			<class name="com.qa.opencart.tests.LoginPageTest" />
		</classes>
	</test>

	<test name="Accounts Page Test">
		<classes>
			<class name="com.qa.opencart.tests.AccountsPageTest" />
		</classes>
	</test>

	<!-- <test name="Product Info Page Test"> <classes> <class name="com.qa.opencart.tests.ProductPageInfoTest" 
		/> </classes> </test> <test name="Register Page Test"> <classes> <class name="com.qa.opencart.tests.RegisterPageTest" 
		/> </classes> </test> -->





</suite>